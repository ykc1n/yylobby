{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts",
    "src/renderer/utils/*",
    "src/renderer/components/**/*",
    "src/main/router"
, "src/renderer/MultiplayerPage.tsx"  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ],
      "@/*": [
        "src/renderer/*"
      ],
      "@/components/*": [
        "src/renderer/components/*"
      ],
      "@/utils/*": [
        "src/renderer/utils/*"
      ]
    }
  }
}
